{
  "name": "press-ui-demo-vue2-pure",
  "version": "2.2.7",
  "private": true,
  "description": "简单、易用的跨端组件库，兼容 Vue2 和 Vue3，同时支持 uni-app和普通 Vue 项目",
  "repository": {
    "type": "git",
    "url": "https://github.com/novlan1/press-ui-demo-vue2-pure"
  },
  "author": "guowangyang",
  "homepage": "https://novlan1.github.io/press-ui-demo-vue2-pure",
  "scripts": {
    "dev": "vue-cli-service serve --serving",
    "build": "npm run build:pure",
    "build:pure": "vue-cli-service build",
    "build:dev": "npm run press:init && PUBLISH_ENV=test vue-cli-service build",
    "preview": "node build/index.js --preview",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "publish": "light-cli publish",
    "publish:prod": "light-cli publish -e prod",
    "analyze": "NODE_ENV=production npm_config_report=true npm run build",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint --fix src/**/*.css",
    "press:init": "node script/watch/init",
    "init": "npm run press:init",
    "deploy:mine": "node script/deploy/deploy-mine",
    "watch": "node script/watch/watch",
    "init:env": "node script/env/rainbow-env"
  },
